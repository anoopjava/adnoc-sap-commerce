<process xmlns="http://www.hybris.de/xsd/processdefinition" start="adnocReturnOrderAprrovedEmailAction" name="adnocReturnOrderOutbound-process"
		processClass="de.hybris.platform.returns.model.ReturnProcessModel" onError="error">

	<action id="adnocReturnOrderAprrovedEmailAction" bean="adnocReturnOrderAprrovedEmailAction">
		<transition name="OK" to="sendEmail"/>
		<transition name="NOK" to="error"/>
	</action>

	<action id="sendEmail" bean="sendEmail">
		<transition name="OK" to="removeSentEmail"/>
		<transition name="NOK" to="error"/>
	</action>

	<action id="removeSentEmail" bean="removeSentEmail">
		<transition name="OK" to="success"/>
		<transition name="NOK" to="error"/>
	</action>


	<end id="error" state="ERROR">Something went wrong.</end>
	<end id="failed" state="FAILED">Could not send registration .</end>
	<end id="success" state="SUCCEEDED">Sent customer registration.</end>

</process>
