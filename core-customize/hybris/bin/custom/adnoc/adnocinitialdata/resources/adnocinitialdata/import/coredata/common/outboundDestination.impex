$outboundAdnocOverdueInvoicesUrl = $config-adnoc.account.overdue.invoices.destinationurl
$outboundAdnocCreditSimulationUrl = $config-adnoc.account.credit.simulation.destinationurl

#SMS Gateway
$smsgatewayClientId = $config-adnoc.smsgateway.clientid
$smsgatewaySecret = $config-adnoc.smsgateway.secret
$smsgatewayAuthUrl = $config-adnoc.smsgateway.authurl
$adnocSmsGatewayDestinationUrl = $config-adnoc.smsgateway.destinationurl
