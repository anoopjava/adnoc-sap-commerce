$adnocReturnDestinationUrl = $config-adnoc.return.destinationurl
$adnocReturnDestinationcsrfUrl = $config-adnoc.return.destination.csrf.endpoint
#CsTicket to CRM
$adnocCrmCsTicketDestinationUrl = $config-adnoc.csticket.tocrm.destinationurl
$adnocCrmCsTicketDestinationcsrfUrl = $config-adnoc.csticket.tocrm.destination.csrf.endpoint

#Payment
$adnocPaymentEndPointUrl = $config-adnoc.payment.card.initiatecheckout
