$adnocOverduePaymentCardEndPointUrl = $config-adnoc.overdue.payment.outbound
$adnocOverduePaymentCardEndPointCsrfUrl = $config-adnoc.overdue.payment.outbound.csrf
$paymentGatewayClientId = $config-adnoc.payment.card.username
$paymentGatewaySecret = $config-adnoc.payment.card.password

#Bank Transfer Changes
$adnocPaymentBankRegistrationEndPoint = $config-adnoc.payment.banktransfer.registration.endpoint
$adnocBankFinalizationEndPoint = $config-adnoc.payment.banktransfer.finalization.endpoint
$adnocBankRegistrationClinetId = $config-adnoc.payment.banktransfer.uaepgs.username
$adnocBankRegistrationSecret = $config-adnoc.payment.banktransfer.uaepgs.password

#CreditLimit
$outboundAdnocB2BCreditLimitEndpointUrl = $config-adnoc.user.payment.credit.limit.destinationurl
#SapPricing
$adnoc_product_sap_pricing_fetch_url = $config-adnoc.product.sap.pricing.fetch.url
#B2BDocumentInvoice
$adnoc_b2binvoice_document_url = $config-adnoc.b2binvoice.document.url

## Gigya Configuration
$gigyaOauthApiKey = $config-adnoc.gigya.apikey
$gigyaOauthClientId = $config-adnoc.gigya.userkey
$gigyaOauthSecret = $config-adnoc.gigya.usersecret
$gigyaOAuthUrl = $config-adnoc.gigya.access.token.authurl

#Login Token Verification
$gigyaLoginVerificationDestinationUrl = $config-adnoc.gigya.login.token.destinationurl

#Gigya Duplicate Check
$gigyaDuplicateCheckEndPoint = $config-adnoc.gigya.duplicatecheck.destinationurl

